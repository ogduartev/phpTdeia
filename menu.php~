      $data['table1']="factors";
      $data['up_id1']="factor_id";
      $data['name1']="name";
      $data['help1']="description";
      $data['link_id1']="login.php";
/*      $data['table2']="factors";
      $data['up_id2']="factor_id";
      $data['name2']="name";
      $data['help2']="description";
      $data['url']="block.php";
      $data['link_id2']="modelid";*/
      $tree = new tlistemysql("_1","","menumodel",$data," AND project_id='".$_POST['TDEIA_project_id']."'") ;
      $tree->display();
    }
  }
}
